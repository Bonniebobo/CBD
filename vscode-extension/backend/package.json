{
  "name": "ai-code-assistant-backend",
  "version": "1.0.0",
  "description": "Backend API for AI Code Assistant VS Code Extension",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest tests/unit --coverage",
    "test:watch": "jest tests/unit --watch",
    "test:integration": "jest tests/integration/api-integration.test.js --forceExit --detectOpenHandles",
    "test:llm-integration": "jest tests/integration/llm-integration.test.js --forceExit --detectOpenHandles",
    "test:all": "npm test && npm run test:integration",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "keywords": [
    "ai",
    "code-assistant",
    "vscode-extension",
    "backend",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.1.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/ai-code-assistant-backend.git"
  },
  "bugs": {
